class Animal{
    constructor(nombre){
        this._nombre=nombre;
    }

    get nombre(){
        return this._nombre;
    }

    set nombre(nombre){
        this._nombre=nombre;
    }
    mensaje(){
        console.log('EEsto es un Mensaje');
    }
}
var objeto1=new Animal('gato');
console.log(objeto1)
objeto1.nombre='gato domestico'
console.log(objeto1.nombre);
